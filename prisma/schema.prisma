generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model howard_hs_2022 {
  fid                                   Int                      @id @default(autoincrement())
  name                                  String?                  @db.VarChar
  description                           String?                  @db.VarChar
  timestamp                             DateTime?                @db.Timestamptz(6)
  begin                                 DateTime?                @db.Timestamptz(6)
  end                                   DateTime?                @db.Timestamptz(6)
  altitudemode                          String?                  @db.VarChar
  tessellate                            Int?
  extrude                               Int?
  visibility                            Int?
  draworder                             Int?
  icon                                  String?                  @db.VarChar
  hi_home                               String?                  @db.VarChar
  street1                               String?                  @db.VarChar
  street2                               String?                  @db.VarChar
  high_school_ratings_usn_md_rank       Int?
  high_school_ratings_usn_national_rank Int?
  high_school_ratings_gs_overall        Int?
  high_school_ratings_gs_tests          Int?
  high_school_ratings_gs_college        Int?
  high_school_ratings_gs_equity         Int?
  high_school_composite_score           Float?
  geom                                  Unsupported("geometry")?

  @@index([geom], map: "howard_hs_2022_geom_geom_idx", type: Gist)
}

model rawZillowData {
  id            Int      @id @default(autoincrement())
  address       String
  area          Int?
  bathrooms     Float?
  bedrooms      Int?
  brokerName    String?
  currency      String?
  daysOnZillow  Int?
  image         String?
  input         String?
  isZillowOwned Boolean?
  latitude      Float?
  listingType   String?
  listingUrl    String?
  longitude     Float?
  price         Int?
  propertyId    String
  propertyUrl   String?
  rank          Int?
  rentZestimate Int?
  zestimate     Int?
  filename      String
  createdAt     DateTime @default(now())

  @@index([filename])
  @@index([propertyId])
}

view ZillowDataView {
  id            Int  @unique
  address       String?
  area          Int?
  bathrooms     Float?
  bedrooms      Int?
  brokerName    String?
  currency      String?
  daysOnZillow  Int?
  image         String?
  input         String?
  isZillowOwned Boolean?
  latitude      Float?
  listingType   String?
  listingUrl    String?
  longitude     Float?
  price         Int?
  propertyId    String?
  propertyUrl   String?
  rank          Int?
  rentZestimate Int?
  zestimate     Int?
  filename      String?
  createdAt     DateTime?
  school_name   String?   @db.VarChar
}
